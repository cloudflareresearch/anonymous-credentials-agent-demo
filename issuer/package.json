{
	"name": "act-issuer-demo",
	"version": "0.1.0",
	"author": "Armando Faz <armfazh@cloudflare.com>",
	"repository": "https://github.com/cloudflareresearch/anonymous-credentials-agent-demo",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"keygen": "cargo run --manifest-path ./keygen/Cargo.toml issuer_key.txt",
		"upload-keygen": "cat issuer_key.txt | wrangler secret put ISSUER_PRIVATEKEY_CBOR_HEX"
	},
	"devDependencies": {
		"wrangler": "4.45.1"
	}
}
